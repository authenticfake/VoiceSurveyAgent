{
  "version": "1.0",
  "req_id": "REQ-002",
  "lane": "python",
  "env": {
    "DATABASE_URL": "sqlite+pysqlite:///:memory:",
    "OIDC_ISSUER": "http://mock-idp",
    "OIDC_CLIENT_ID": "client-123",
    "OIDC_CLIENT_SECRET": "secret-abc",
    "OIDC_AUTHORIZATION_ENDPOINT": "http://mock-idp/authorize",
    "OIDC_TOKEN_ENDPOINT": "http://mock-idp/token",
    "OIDC_USERINFO_ENDPOINT": "http://mock-idp/userinfo",
    "OIDC_REDIRECT_URI": "http://localhost/callback",
    "AUTH_TOKEN_SECRET": "supersecuresecretkeyvalue123456789"
  },
  "commands": {
    "tests": "pytest -q runs/kit/REQ-002/test"
  },
  "cases": [
    {
      "name": "tests",
      "run": "pytest -q runs/kit/REQ-002/test",
      "cwd": ".",
      "pip-file": "runs/kit/REQ-002/src/requirements.txt",
      "expect": 0,
      "timeout": 600
    }
  ],
  "tools": {
    "tests": true,
    "lint": false,
    "types": false,
    "security": false,
    "build": false
  },
  "reports": [],
  "gate_policy": {
    "tests_pass": true,
    "coverage_min": 0.8,
    "security": {
      "bandit_high": 0
    }
  },
  "normalize": {
    "summary": "eval.summary.json"
  },
  "constraints_applied": {
    "tech_stack": "fastapi/python",
    "db_engine": "sqlalchemy-sync",
    "oidc_flow": "authorization_code",
    "jwt_algorithm": "HS256"
  }
}